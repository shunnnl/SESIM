            "uid": "a8ed462a-5454-4bc8-ac91-9eccf463b5ad"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH logs AS (\r\n  SELECT \r\n    is_attack,\r\n    status_code,\r\n    path\r\n  FROM ai_result \r\n  WHERE timestamp BETWEEN to_timestamp($__from / 1000) AND to_timestamp($__to / 1000)\r\n),\r\naggregates AS (\r\n  SELECT \r\n    COUNT(*) AS total_requests,\r\n    SUM(CASE WHEN is_attack THEN 1 ELSE 0 END) AS attack_requests,\r\n    SUM(CASE WHEN status_code BETWEEN 500 AND 599 THEN 1 ELSE 0 END) AS error_5xx,\r\n    SUM(CASE WHEN path ILIKE '%../%' OR path ILIKE '%admin%' OR path ILIKE '%.php%' THEN 1 ELSE 0 END) AS abnormal_paths\r\n  FROM logs\r\n),\r\nscores AS (\r\n  SELECT\r\n    total_requests,\r\n    attack_requests,\r\n    error_5xx,\r\n    abnormal_paths,\r\n    attack_requests::float / NULLIF(total_requests, 0) AS attack_rate,\r\n    CASE \r\n      WHEN total_requests = 0 THEN 100\r\n      ELSE GREATEST(\r\n        0,\r\n        100\r\n        - CASE \r\n            WHEN attack_requests::float / NULLIF(total_requests, 0) >= 0.2 THEN attack_requests::float / NULLIF(total_requests, 0) * 60\r\n            ELSE attack_requests::float / NULLIF(total_requests, 0) * 30\r\n          END\r\n        - (error_5xx::float / total_requests) * 25\r\n        - (abnormal_paths::float / total_requests) * 15\r\n      )\r\n    END AS score\r\n  FROM aggregates\r\n)\r\nSELECT\r\n  score AS \"보안점수\",\r\n  CASE\r\n    WHEN score >= 90 THEN '매우 양호'\r\n    WHEN score >= 70 THEN '양호,약간 주의 필요'\r\n    WHEN score >= 50 THEN '경고'\r\n    WHEN score >= 30 THEN '위험'\r\n    ELSE '심각'\r\n  END AS \"보안 상태\"\r\nFROM scores;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "보안점수",
      "type": "stat"
