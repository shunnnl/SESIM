          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH interval_setting AS (\r\n    SELECT \r\n        CASE \r\n            WHEN (${__to} - ${__from}) <= 3600000 THEN '1 minute'::interval  -- 1시간 이하\r\n            WHEN (${__to} - ${__from}) <= 86400000 THEN '1 hour'::interval    -- 24시간 이하\r\n            ELSE '1 day'::interval  -- 24시간 초과\r\n        END AS interval_value,\r\n        CASE \r\n            WHEN (${__to} - ${__from}) <= 3600000 THEN 'minute'  -- 1시간 이하\r\n            WHEN (${__to} - ${__from}) <= 86400000 THEN 'hour'   -- 24시간 이하\r\n            ELSE 'day'  -- 24시간 초과\r\n        END AS trunc_unit\r\n),\r\ntime_series AS (\r\n    SELECT generate_series(\r\n        date_trunc(\r\n            (SELECT trunc_unit FROM interval_setting),\r\n            to_timestamp(${__from} / 1000)\r\n        ),\r\n        date_trunc(\r\n            (SELECT trunc_unit FROM interval_setting),\r\n            to_timestamp(${__to} / 1000)\r\n        ),\r\n        (SELECT interval_value FROM interval_setting)\r\n    ) AS time_interval\r\n),\r\nattack_data AS (\r\n    SELECT\r\n        CASE\r\n            WHEN (SELECT interval_value FROM interval_setting) = '1 minute'::interval\r\n                THEN date_trunc('minute', timestamp)  -- 1분 단위 처리\r\n            WHEN (SELECT interval_value FROM interval_setting) = '10 minutes'::interval\r\n                THEN date_trunc('hour', timestamp) + INTERVAL '10 min' * floor(date_part('minute', timestamp) / 10)  -- 10분 단위 처리\r\n            WHEN (SELECT interval_value FROM interval_setting) = '1 hour'::interval\r\n                THEN date_trunc('hour', timestamp)  -- 1시간 단위 처리\r\n            ELSE\r\n                date_trunc('day', timestamp)  -- 1일 단위 처리\r\n        END AS time_interval,\r\n        COUNT(*) AS anomaly_count\r\n    FROM ai_result \r\n    WHERE is_attack = TRUE\r\n      AND timestamp >= to_timestamp(${__from} / 1000)\r\n      AND timestamp <= to_timestamp(${__to} / 1000)\r\n    GROUP BY time_interval\r\n)\r\nSELECT \r\n    ts.time_interval,\r\n    COALESCE(ad.anomaly_count, 0) AS anomaly_count\r\nFROM time_series ts\r\nLEFT JOIN attack_data ad ON ts.time_interval = ad.time_interval\r\nORDER BY ts.time_interval;\r\n",
